/* pages/logs_Bio_yuyue_city/logs_Bio_yuyue_city.wxss */
.pages{
  width: 90%;
  margin: 0 auto;
}
.here{
  width: 100%;
  height: 170rpx;
}
.here text:nth-child(1){
  display: block;
  color: rgb(126, 126, 126);
  font-size: 26rpx;
}
.here text:nth-child(2){
  display: block;
  font-size: 50rpx;
  margin-top: 30rpx;
}
.city_list{
  margin-top: 20rpx;
  width: 100%;
}
.city_list text{
  font-size: 36rpx;
}
.citys{
  margin-top: 10rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.city{
  width: 200rpx;
  height: 80rpx;
  margin-top: 20rpx;
  border-radius: 8rpx;
  text-align: center;
  line-height: 90rpx;
  font-size: 24rpx;
  color: rgb(73, 73, 73);
  background-color: rgb(247, 245, 242);
}
